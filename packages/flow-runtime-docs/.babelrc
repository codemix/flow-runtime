{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "exclude": [
        "transform-regenerator"
      ]
    }],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    [ '@babel/plugin-proposal-decorators', { legacy: true } ],
    [ '@babel/plugin-proposal-class-properties', { loose: true } ],
    ["flow-runtime", {warn: true, annotate: true}]
  ]
}
