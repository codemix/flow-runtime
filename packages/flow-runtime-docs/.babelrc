{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "exclude": [
        "transform-regenerator"
      ]
    }],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/proposal-decorators-legacy", {"legacy": true}],
    ["babel-plugin-flow-runtime", {
      "warn": true,
      "annotate": true
    }]
  ]
}
