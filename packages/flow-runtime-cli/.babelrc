{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": 4
        },
        "exclude": [
          "transform-regenerator"
        ]
      }
    ],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@Babel/preset-env",
          {
            "targets": {
              "node": 4
            },
            "exclude": [
              "transform-regenerator"
            ]
          }
        ],
        "@babel/preset-flow",
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        ["@babel/proposal-decorators-legacy", {"legacy": true}],
        ["babel-plugin-flow-runtime", {
          "warn": true,
          "annotate": true
        }]
      ],
    }
  },
  "passPerPreset": true
}
